{
    "extends": "rallycoding",
    "rules": {
        "arrow-body-style": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "max-len":0,
        "prefer-template":0,
        "object-shorthand":0,
        "react/jsx-boolean-value":0,
        "global-require":0,
        "no-underscore-dangle":0,
        "no-param-reassign":0,
        "dot-notation":0,
        "brace-style":0,
        "no-unneeded-ternary": 0,
        "key-spacing": 0
    }
}